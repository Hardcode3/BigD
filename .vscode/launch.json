{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Serve Doc",
            "consoleName": "Mkdocs Documentation Serve",
            "request": "launch",
            "type": "debugpy",
            "module": "mkdocs",
            "args": [
                "serve"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "preLaunchTask": "Open Browser at 127.0.0.1:8000"
        },
        {
            "name": "Build Doc",
            "consoleName": "Mkdocs Documentation Build",
            "request": "launch",
            "type": "debugpy",
            "module": "mkdocs",
            "args": [
                "build"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
        
    ]
}